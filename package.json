{
  "name": "rhs-sdk",
  "displayName": "rhs-sdk",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onCommand:configurator.editor",
    "onView:rhs-sdk.tools"
  ],
  "contributes": {
    "commands": [
      {
        "command": "rhs-sdk.tools.open",
        "title": "Open Tool Page",
        "category": "Tools"
      },
      {
        "command": "rhs-sdk.tools.creator",
        "title": "Open Page 1",
        "icon": "$(symbol-string)"
      },
      {
        "command": "rhs-sdk.tools.editor",
        "title": "Open Page 1",
        "icon": "$(symbol-string)"
      },
      {
        "command": "rhs-sdk.tools.logger",
        "title": "Open Page 1",
        "icon": "$(symbol-string)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rhs-sdk-sidebar",
          "title": "RHS SDK",
          "icon": "src/media/RHS.svg"
        }
      ]
    },
    "views": {
      "rhs-sdk-sidebar": [
        {
          "id": "rhs-sdk.tools",
          "name": "Tools"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "esbuild": "^0.25.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}